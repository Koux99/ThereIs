<% if user_signed_in? %>
 <% if current_user.liked_by?(post.id) %>
   <td><%= link_to 'いいねを外す', post_like_path, method: :delete, remote: true %> <%= post.likes.count %></td>
 <% else %>
   <td><%= link_to 'いいね', post_likes_path, method: :post, remote: true %> <%= post.likes.count %></td>
 <% end %>
<% end %>